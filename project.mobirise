              }
            },
            ".form-container when not (@transparentBg)": {
              ".mbr-overlay": {
                "background": "@cardBg",
                "opacity": "@cardOpacity"
              },
              ".image-wrap": {
                "padding": "0"
              }
            },
            "amp-img,img": {
              "object-fit": "cover",
              "@media (min-width: 768px)": {
                "height": "~\"@{heightImg}rem\""
              }
            },
            "input, textarea": {
              "color": "@labelsColor",
              "background": "contrast(@labelsColor, darken(@labelsColor, 50%), #ffffff, 60%) !important",
              "border": "1px solid lighten(@labelsColor, 88%)",
              "&:focus": {
                "border-color": "@secondaryColor"
              }
            },
            "input::-webkit-input-placeholder": {
              "color": "lighten(@labelsColor, 46%)"
            },
            "input::-moz-placeholder": {
              "color": "lighten(@labelsColor, 46%)"
            },
            "input::placeholder": {
              "color": "lighten(@labelsColor, 46%)"
            },
            "textarea::-webkit-input-placeholder": {
              "color": "lighten(@labelsColor, 46%)"
            },
            "textarea::-moz-placeholder": {
              "color": "lighten(@labelsColor, 46%)"
            },
            "textarea::placeholder": {
              "color": "lighten(@labelsColor, 46%)"
            },
            ".fieldset > .split-row": {
              "input": {
                "display": "flex",
                "flex": "1 1 50%",
                "@media (min-width: 768px)": {
                  "margin-left": "15px",
                  "margin-right": "15px"
                }
              },
              "@media (max-width: 767px)": {
                "flex-direction": "column"
              }
            },
            ".marker-wrap:before when not (@stylizedCounters) and (@showFormList)": {
              "content": "\"\"",
              "display": "inline-block",
              "border": "3px solid @primaryColor",
              "width": "~\"calc(@{widthImg}rem - 6px)\"",
              "height": "~\"calc(@{widthImg}rem - 6px)\"",
              "box-sizing": "initial"
