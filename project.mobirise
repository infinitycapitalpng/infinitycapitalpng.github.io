            "& when (@showPadding)": {
              ".form-container": {
                "@media (min-width: 991px)": {
                  ".form-wrap": {
                    "padding-left": "70px"
                  },
                  "&.mbr-row-reverse .form-wrap": {
                    "padding-right": "70px",
                    "padding-left": "15px"
                  }
                }
              },
              "@media (min-width: 1200px)": {
                ".container-fluid .mbr-row .form-wrap": {
                  "padding-left": "100px",
                  "padding-right": "100px"
