            "& when (@bg-type = 'color')": {
              "background-color": "@bg-value",
              ".mbr-overlay when (@twoBgColor)": {
                "padding": "0",
                "background": "@twoColor",
                "opacity": "1"
              }
            },
            "& when (@bg-type = 'image')": {
              "background-image": "url(@bg-value)",
              ".mbr-overlay": {
                "background": "@overlayColor",
                "opacity": "@overlayOpacity"
              }
            },
            ".card-box when (@showCardsTitle), (@showCardsSubtitle), (@showCardsText), (@showCardsButtons)": {
              "@media (min-width: 768px)": {
                "padding-left": "45px",
                "padding-right": "45px"
              },
              "@media (min-width: 1200px)": {
                "& when (@fullWidth)": {
                  "padding-left": "140px",
                  "padding-right": "140px"
                }
              },
              "@media (max-width: 767px)": {
                "& when (@showImg)": {
